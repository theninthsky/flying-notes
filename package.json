{
  "name": "flying-notes",
  "version": "1.0.0",
  "description": "an application for taking notes.",
  "engines": {
    "node": "13.9.0"
  },
  "main": "server/src/server.js",
  "scripts": {
    "start": "cd server && npm start",
    "app": "cd client && npm run build && cd ../server && npm start",
    "test:server": "cd server && npm test",
    "install:server": "cd server && npm i",
    "install:client": "cd client && npm i && npm run build",
    "deploy": "npm run test:server && git push origin master",
    "heroku-postbuild": "npm run install:server & npm run install:client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theninthsky/flying-notes.git"
  },
  "author": "Almog Gabay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theninthsky/flying-notes/issues"
  },
  "homepage": "https://github.com/theninthsky/flying-notes#readme"
}
