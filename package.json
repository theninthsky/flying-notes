{
  "name": "flying-notes",
  "version": "1.0.0",
  "description": "an application for taking notes.",
  "engines": {
    "node": "13.7.0"
  },
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "dev": "nodemon server",
    "client": "cd client && npm start",
    "app": "cd client && npm run build && cd .. && npm start",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "npm run start:test & jest",
    "start:test": "NODE_ENV=test node server",
    "test:watch": "WATCH=true jest --watch",
    "deploy": "npm test && git push origin master & git push heroku master",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "author": "Almog Gabay",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "mongoose": "^5.9.4"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "mongodb-memory-server": "^6.3.2",
    "nodemon": "^2.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "client/**"
    ]
  },
  "eslintIgnore": [
    "client"
  ],
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theninthsky/flying-notes.git"
  },
  "bugs": {
    "url": "https://github.com/theninthsky/flying-notes/issues"
  },
  "homepage": "https://github.com/theninthsky/flying-notes#readme"
}
